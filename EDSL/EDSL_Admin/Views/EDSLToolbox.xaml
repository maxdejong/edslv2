<Page x:Class="EDSL_Admin.Views.EDSLToolbox"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:EDSL_Admin.Views"
      mc:Ignorable="d" 
      d:DesignHeight="1000" Width="1920"
      Title="EDSLToolbox">

    <Grid>
        <Button x:Name="Homebtn" Content="Home" HorizontalAlignment="Left" VerticalAlignment="Top" Width="119" Height="80" FontSize="22" Click="Homebtn_Click"/>
        <Frame x:Name="Display" Content="" HorizontalAlignment="Left" Height="920" VerticalAlignment="Bottom" Width="1920" NavigationUIVisibility="Hidden"/>
        <Label x:Name="Title" Content="Label" HorizontalAlignment="Left" Margin="774,0,0,0" VerticalAlignment="Top" Height="80" Width="298" FontSize="40"/>
        <Button x:Name="SeasonBtn" Content="Season" HorizontalAlignment="Left" Margin="119,0,0,0" VerticalAlignment="Top" Width="119" Height="80" FontSize="22" Click="SeasonBtn_Click"/>
        <Button x:Name="ClubTeamBtn" Content="Club/Team" HorizontalAlignment="Left" Margin="238,0,0,0" VerticalAlignment="Top" Width="119" Height="80" FontSize="22"/>

    </Grid>
</Page>
